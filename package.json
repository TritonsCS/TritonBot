{
  "name": "tritonauthenticator",
  "version": "1.0.0",
  "description": "Discord bot that verifies if users are students at Edmonds College",
  "main": "bot.js",
  "scripts": {
    "build": "yarn clean && tsc && yarn copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles resources/* dist/utils/ && copyfiles config.json .env dist/",
    "start:dev": "ts-node ./src/bot.ts",
    "start:prod": "node ./dist/bot.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TritonVerifier"
  },
  "author": "_Pesto",
  "license": "ISC",
  "dependencies": {
    "@types/chromedriver": "^81.0.1",
    "@types/node": "^18.7.19",
    "@types/selenium-webdriver": "^4.1.5",
    "chromedriver": "^106.0.0",
    "copyfiles": "^2.4.1",
    "discord.js": "^14.4.0",
    "dotenv": "^16.0.3",
    "moment": "^2.29.4",
    "rimraf": "^3.0.2",
    "selenium-webdriver": "^4.5.0",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "tspath": "^2.5.7",
    "typescript": "^4.8.3",
    "yarn": "^1.22.19"
  }
}
