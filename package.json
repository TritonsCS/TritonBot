{
  "name": "tritonbot",
  "version": "1.0.0",
  "description": "Discord bot for the Edmonds College CS Club.",
  "main": "bot.js",
  "scripts": {
    "build": "yarn clean && tsc && yarn copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles resources/* dist/utils/ && copyfiles config.json .env dist/",
    "start:dev": "ts-node ./src/bot.ts",
    "start:prod": "node ./dist/bot.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devPesto/TritonBot"
  },
  "author": "_Pesto",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.11.30",
    "axios": "^1.6.5",
    "copyfiles": "^2.4.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.0",
    "moment": "^2.30.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tspath": "^2.6.8",
    "typescript": "^5.4.2",
    "yarn": "^1.22.21"
  }
}
