{
  "name": "tritonbot",
  "version": "1.0.0",
  "description": "Discord bot for the Edmonds College CS Club.",
  "main": "bot.js",
  "scripts": {
    "build": "yarn clean && tsc && yarn copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles resources/* dist/utils/ && copyfiles config.json .env dist/",
    "start:dev": "ts-node ./src/bot.ts",
    "start:prod": "node ./dist/bot.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devPesto/TritonBot"
  },
  "author": "_Pesto",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.2.1",
    "axios": "^1.3.4",
    "copyfiles": "^2.4.1",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "form-data": "^4.0.0",
    "moment": "^2.29.4",
    "rimraf": "^4.3.0",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "tspath": "^2.6.8",
    "typescript": "^4.9.5",
    "yarn": "^1.22.19"
  }
}
